{
  "name": "kosmic-template",
  "version": "0.0.0",
  "description": "A highly opinionated and strongly typed Koa web and api template.",
  "bugs": {
    "url": "https://github.com/spence-s/kosmic/issues"
  },
  "repository": {
    "url": "https://github/com/spence-s/kosmic"
  },
  "license": "MIT",
  "author": {
    "name": "Spencer Snyder",
    "email": "sasnyde2@gmail.com",
    "url": "https://spencersnyder.io"
  },
  "type": "module",
  "imports": {
    "#components/*": "./dist/src/components/*",
    "#config/*": "./dist/src/config/*",
    "#models/*": "./dist/src/models/*",
    "#middleware/*": "./dist/src/middleware/*",
    "#db/*": "./dist/src/db/*",
    "#utils/*": "./dist/src/utils/*",
    "#server": "./dist/src/server.js"
  },
  "main": "dist/src/index.js",
  "source": "./src/app/client/main.ts",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "analyze": "vite-bundle-visualizer",
    "build": "npm run clean && npm run compile",
    "build:vite-plugin": "tsc -p ./scripts/tsconfig.json",
    "check": "tsc -p tsconfig.json --noEmit",
    "check:watch": "tsc -p tsconfig.json --noEmit --watch",
    "clean": "rimraf -rf ./dist",
    "compile": "npm run compile:tsc && npm run compile:vite && npm run compile:cp",
    "compile:cp": "cp -rp src/public ./dist/src",
    "compile:tsc": "tsc -p tsconfig.json",
    "compile:vite": "vite build",
    "dev": "node --no-warnings --experimental-strip-types scripts/dev.ts",
    "lint": "xo",
    "migrate": "npm run compile:tsc && node ./dist/src/db/migrate up",
    "migrate:down": "npm run compile:tsc && node ./dist/src/db/migrate down",
    "prepare": "husky || true",
    "prepublishOnly": "npm run build",
    "start": "NODE_ENV=production node dist/src/index.js",
    "test": "npm run clean && npm run compile:tsc && node --test dist/test/server.test.js"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-packagejson",
      "prettier-plugin-organize-attributes"
    ]
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@stripe/stripe-js": "^6.1.0",
    "argon2": "^0.41.1",
    "bootstrap": "^5.3.3",
    "camelcase": "^8.0.0",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "dedent": "^1.5.3",
    "defaults": "^3.0.0",
    "dotenv": "^16.4.7",
    "execa": "^9.5.2",
    "globby": "^14.1.0",
    "got": "^14.4.6",
    "helmet": "^8.1.0",
    "highlight.js": "^11.11.1",
    "htmx.org": "^1.9.12",
    "humanize-string": "^3.0.0",
    "kcors": "^2.2.2",
    "koa": "^2.16.0",
    "koa-404-handler": "^0.1.0",
    "koa-bodyparser": "^4.4.1",
    "koa-compose": "^4.1.0",
    "koa-conditional-get": "^3.0.0",
    "koa-connect": "^2.1.0",
    "koa-etag": "^4.0.0",
    "koa-json": "^2.0.2",
    "koa-passport": "^6",
    "koa-pino-logger": "^4.0.0",
    "koa-requestid": "^2.2.1",
    "koa-response-time": "^2.1.0",
    "koa-session": "^7.0.2",
    "koa-static": "^5.0.0",
    "kysely": "^0.27.6",
    "p-debounce": "^4.0.0",
    "p-queue": "^8.1.0",
    "p-wait-for": "^5.0.2",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "path-exists": "^5.0.0",
    "path-to-regexp": "^8.2.0",
    "pg": "^8.14.1",
    "pino": "9.6.0",
    "pino-http": "^10.4.0",
    "pino-princess": "^1.0.0",
    "preact": "^10.26.4",
    "preact-render-to-string": "^6.5.13",
    "queue": "^7.0.0",
    "short-uuid": "^5.2.0",
    "stripe": "^17.7.0",
    "titleize": "^4.0.0",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@kosmic/types": "^0.1.0",
    "@types/bootstrap": "^5.2.10",
    "@types/kcors": "^2",
    "@types/koa-bodyparser": "^4",
    "@types/koa-conditional-get": "^2.0.3",
    "@types/koa-etag": "^3.0.3",
    "@types/koa-passport": "^6.0.3",
    "@types/koa-pino-logger": "^3.0.4",
    "@types/koa-response-time": "^2",
    "@types/koa-session": "^6.4.5",
    "@types/koa-static": "^4.0.4",
    "@types/node": "~22.13.14",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.11.11",
    "@types/picomatch": "^3.0.2",
    "@types/swagger-ui-dist": "^3.30.5",
    "@types/wait-on": "^5.3.4",
    "chokidar": "^4.0.3",
    "husky": "^9.1.7",
    "kosmic": "file:.",
    "lint-staged": "^15.5.0",
    "meow": "^13.2.0",
    "npm-package-json-lint": "^8.0.0",
    "npm-package-json-lint-config-default": "^7.0.1",
    "picocolors": "^1.1.1",
    "picomatch": "^4.0.2",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-packagejson": "^2.5.10",
    "rimraf": "^6.0.1",
    "sass": "1.86.0",
    "swagger-ui-dist": "^5.20.2",
    "type-fest": "^4.38.0",
    "typed-htmx": "^0.3.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.3",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-tsc-watch": "^3.1.0",
    "wait-on": "^8.0.3",
    "xo": "npm:@spence-s/flat-xo@^0.2.1"
  }
}
