{
  "name": "kosmic-template",
  "version": "0.0.0",
  "description": "A highly opinionated and strongly typed Koa web and api template.",
  "bugs": {
    "url": "https://github.com/spence-s/kosmic/issues"
  },
  "repository": {
    "url": "https://github/com/spence-s/kosmic"
  },
  "license": "MIT",
  "author": {
    "name": "Spencer Snyder",
    "email": "sasnyde2@gmail.com",
    "url": "https://spencersnyder.io"
  },
  "type": "module",
  "imports": {
    "#components/*": "./dist/src/components/*",
    "#config/*": "./dist/src/config/*",
    "#models/*": "./dist/src/models/*",
    "#middleware/*": "./dist/src/middleware/*",
    "#db/*": "./dist/src/db/*",
    "#emails/*": "./dist/src/emails/*",
    "#utils/*": "./dist/src/utils/*",
    "#islands/*": "./dist/src/client/islands/*",
    "#server": "./dist/src/server.js"
  },
  "main": "dist/src/index.js",
  "source": "./src/app/client/main.ts",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "analyze": "vite-bundle-visualizer",
    "build": "npm run clean && npm run compile",
    "build:vite-plugin": "tsc -p ./scripts/tsconfig.json",
    "check": "tsc -p tsconfig.json --noEmit",
    "check:watch": "tsc -p tsconfig.json --noEmit --watch",
    "clean": "rimraf dist",
    "compile": "npm run compile:tsc && npm run compile:vite && npm run compile:cp",
    "compile:cp": "cp -rp src/public ./dist/src",
    "compile:tsc": "tsc -p tsconfig.json",
    "compile:vite": "vite build",
    "dev": "node --no-warnings --experimental-strip-types --enable-source-maps scripts/dev.ts",
    "lint": "xo",
    "migrate": "npm run clean && npm run compile:tsc && KOSMIC_ENV=migration node --enable-source-maps ./dist/src/db/utils/cli.js",
    "prepare": "husky || true",
    "prepublishOnly": "npm run build",
    "start": "NODE_ENV=production KOSMIC_ENV=production node dist/src/index.js | pino-princess",
    "test": "rimraf dist && tsc -p tsconfig.json & wait-on dist/test/server.test.js && NODE_ENV=test KOSMIC_ENV=test node --test dist/test/server.test.js",
    "test:watch": "rimraf dist && tsc -p tsconfig.json --watch & wait-on dist/test/server.test.js && NODE_ENV=test KOSMIC_ENV=test node --test --watch dist/test/server.test.js"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-packagejson",
      "prettier-plugin-organize-attributes"
    ]
  },
  "overrides": {
    "zod": "$zod"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@google/genai": "^1.14.0",
    "@koa/etag": "^5.0.1",
    "@popperjs/core": "^2.11.8",
    "@stripe/stripe-js": "^7.8.0",
    "argon2": "^0.44.0",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "camelcase": "^8.0.0",
    "cheerio": "^1.1.2",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "cron": "^4.3.3",
    "dedent": "^1.6.0",
    "dotenv": "^17.2.1",
    "execa": "^9.6.0",
    "globby": "^14.1.0",
    "got": "^14.4.7",
    "helmet": "^8.1.0",
    "highlight.js": "^11.11.1",
    "htmx.org": "^2.0.6",
    "humanize-string": "^3.0.0",
    "juice": "^11.0.1",
    "koa": "^3.0.1",
    "koa-bodyparser": "^4.4.1",
    "koa-compose": "^4.1.0",
    "koa-conditional-get": "^3.0.0",
    "koa-passport": "^6",
    "koa-pino-logger": "^4.0.0",
    "koa-response-time": "^2.1.0",
    "koa-session": "^6.4.0",
    "koa-static": "^5.0.0",
    "kysely": "^0.28.5",
    "nodemailer": "^7.0.5",
    "openai": "^5.12.2",
    "p-wait-for": "^5.0.2",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "path-exists": "^5.0.0",
    "path-to-regexp": "^8.2.0",
    "pg": "^8.16.3",
    "pino": "9.9.0",
    "pino-http": "^10.5.0",
    "pino-princess": "^1.2.0",
    "postcss": "^8.5.6",
    "postcss-css-variables": "^0.19.0",
    "preact": "^10.27.0",
    "preact-render-to-string": "^6.5.13",
    "pretty": "^2.0.0",
    "preview-email": "^3.1.0",
    "stripe": "^18.4.0",
    "titleize": "^4.0.0",
    "tough-cookie": "^6.0.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/bootstrap": "^5.2.10",
    "@types/koa": "^3.0.0",
    "@types/koa-bodyparser": "^4",
    "@types/koa-compose": "^3.2.8",
    "@types/koa-conditional-get": "^2.0.3",
    "@types/koa-passport": "^6.0.3",
    "@types/koa-pino-logger": "^3.0.5",
    "@types/koa-response-time": "^2",
    "@types/koa-session": "^6.4.5",
    "@types/koa-static": "^4.0.4",
    "@types/node": "~24.3.0",
    "@types/nodemailer": "^7.0.0",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-http-bearer": "^1.0.41",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.15.5",
    "@types/picomatch": "^4.0.2",
    "@types/postcss-css-variables": "^0.18.3",
    "@types/pretty": "^2.0.3",
    "@types/preview-email": "^3.1.0",
    "@types/swagger-ui-dist": "^3.30.6",
    "@types/wait-on": "^5.3.4",
    "chokidar": "^4.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "meow": "^13.2.0",
    "npm-check-updates": "^18.0.2",
    "picocolors": "^1.1.1",
    "picomatch": "^4.0.3",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-packagejson": "^2.5.19",
    "rimraf": "^6.0.1",
    "sass": "1.90.0",
    "swagger-ui-dist": "^5.27.1",
    "type-fest": "^4.41.0",
    "typed-htmx": "^0.3.1",
    "typescript": "~5.9.2",
    "vite": "^7.1.2",
    "vite-bundle-visualizer": "^1.2.1",
    "wait-on": "^8.0.4",
    "xo": "^1.2.2"
  }
}
